{
	"debug": false,
	"additional-repositories": ["multilib"],
	"audio": "pipewire",
	"bootloader": "grub-install",
	"custom-commands": [
		"sed -i 's/^#ParallelDownloads = 5/ParallelDownloads = 25/' /etc/pacman.conf",
		"sed -i 's/^#IgnorePkg   =/IgnorePkg=xterm/' /etc/pacman.conf",
		"sed -i 's/^#Color/Color/' /etc/pacman.conf"
	],
	"disk_encryption": null,
	"kernels": ["linux"],
	"keyboard-layout": "us",
	"mirror-region": "Australia",
	"ntp": true,
	"no_pkg_lookups": true,
	"offline": false,
	"plugin": "https://raw.githubusercontent.com/chrishazfun/archinstall-aur/main/archinstall_aur/__init__.py",
	"packages": [
		"bash", "bash-completion", "nano", "vim", "git", "jre-openjdk", "flatpak", "networkmanager", "wget", "curl", "python", "python-pip",

		"lightdm", "lightdm-gtk-greeter", "lightdm-gtk-greeter-settings",
		"exo", "garcon", "tumbler", "xfce4-appfinder", "xfce4-panel", "xfce4-power-manager", "xfce4-session", "xfce4-settings", "xfce4-terminal", "xfconf", "xfdesktop", "xfwm4", "xfwm4-themes", "featherpad", "xfce4-notifyd", "xfce4-screensaver", "xfce4-screenshooter", "xfce4-smartbookmark-plugin", "xfce4-taskmanager",
		"xfce4-battery-plugin", "xfce4-clipman-plugin", "xfce4-genmon-plugin", "xfce4-mount-plugin", "xfce4-mpc-plugin", "xfce4-notes-plugin", "xfce4-pulseaudio-plugin", "xfce4-weather-plugin", "xfce4-whiskermenu-plugin", "xfce4-xkb-plugin",
		"thunar", "thunar-archive-plugin", "thunar-media-tags-plugin", "thunar-volman",
		"xdg-user-dirs", "xdg-desktop-portal",

		"qbittorrent", "dbeaver",
		"meld", "picard", "homebank",
		"solaar", "vlc", "obs-studio", "inkscape", "ardour", "blender", "easyeffects", "steam",
		"yt-dlp", "distrobox",
		"wine-gecko", "wine-mono", "wine-staging",
		"bluez", "bluez-libs", "bluez-utils",
		"qt6-multimedia-ffmpeg", "webp-pixbuf-loader", "ttf-croscore", "calf", "mda.lv2", "lsp-plugins"
	],
	"skip-mirror-check": false,
	"profile": "xorg",
	"services": [
		"lightdm",
		"bluetooth",
		"NetworkManager"
	],
	"swap": true,
	"silent": false,
	"sys-encoding": "utf-8",
	"sys-language": "en_US",
	"timezone": "Australia/Sydney"
}