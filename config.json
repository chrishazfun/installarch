{
	"additional-repositories": [
		"multilib"
	],
	"custom-commands": [
		"sed -i 's/^#ParallelDownloads = 5/ParallelDownloads = 25/' /etc/pacman.conf",
		"sed -i 's/^#IgnorePkg   =/IgnorePkg=xterm/' /etc/pacman.conf",
		"sed -i 's/^#Color/Color/' /etc/pacman.conf"
	],
	"archinstall-language": "English",
	"audio_config": {
		"audio": "pipewire"
	},
	"bootloader": "Grub",
	"config_version": "2.6.3",
	"debug": false,
	"disk_config": {
		"config_type": "default_layout",
		"device_modifications": [
			{
				"device": "/dev/sda"
			}
		]
	},
	"hostname": "changethisplease",
	"kernels": [
		"linux"
	],
	"locale_config": {
		"kb_layout": "us",
		"sys_enc": "UTF-8",
		"sys_lang": "en_US"
	},
	"mirror_config": {
		"custom_mirrors": [],
		"mirror_regions": {
			"Worldwide": [
				"https://geo.mirror.pkgbuild.com/$repo/os/$arch"
			]
		}
	},
	"network_config": {
		"type": "nm"
	},
	"no_pkg_lookups": false,
	"ntp": true,
	"offline": false,
	"packages": [
		"bash", "bash-completion", "nano", "git", "jre-openjdk", "flatpak", "wget", "curl", "python", "python-pipx",
		"ly", "lximage-qt", "lxqt-admin", "lxqt-archiver", "lxqt-config", "lxqt-globalkeys", "lxqt-notificationd", "lxqt-openssh-askpass", "lxqt-panel", "lxqt-policykit", "lxqt-powermanagement", "lxqt-qtplugin", "lxqt-runner", "lxqt-session", "lxqt-sudo", "lxqt-themes", "obconf-qt", "openbox", "pavucontrol-qt", "pcmanfm-qt", "qterminal", "screengrab", "breeze-icons",
		"xdg-desktop-portal-lxqt", "xdg-utils",
		"yt-dlp", "distrobox",
		"toot",
		"streamlink",
		"wine-gecko", "wine-mono", "wine-staging",
		"firefox", "qbittorrent",
		"meld", "picard", "homebank",
		"mpv", "obs-studio", "gimp", "ardour", "blender", "steam",
		"bluez", "bluez-libs", "bluez-utils",
		"qt6-multimedia-ffmpeg", "webp-pixbuf-loader", "ttf-croscore", "ffmpegthumbnailer"
	],
	"services": [
		"ly",
		"bluetooth"
	],
	"parallel downloads": 25,
	"profile_config": {
		"gfx_driver": "All open-source",
		"greeter": null,
		"profile": {
			"custom_settings": {},
			"details": [],
			"main": "Xorg"
		}
	},
	"script": "guided",
	"silent": false,
	"swap": true,
	"skip-mirror-check": true,
	"timezone": "Australia/Sydney",
	"version": "2.6.3"
}