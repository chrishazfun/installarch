{
	"debug": false,
	"additional-repositories": ["multilib"],
	"audio": "pipewire",
	"bootloader": "grub-install",
	"hostname": "archlinux",
	"custom-commands": [
		"sed -i 's/^#ParallelDownloads = 5/ParallelDownloads = 25/' /etc/pacman.conf",
		"sed -i 's/^#IgnorePkg   =/IgnorePkg=xterm/' /etc/pacman.conf",
		"sed -i 's/^#Color/Color/' /etc/pacman.conf"
	],
	"disk_encryption": null,
	"kernels": ["linux"],
	"keyboard-layout": "us",
	"mirror-region": "Australia",
	"ntp": true,
	"no_pkg_lookups": false,
	"offline": false,
	"plugin": "https://raw.githubusercontent.com/chrishazfun/archinstall-mf/main/archinstall_plugin/__init__.py",
	"packages": [
		"bash", "bash-completion", "nano", "vim", "git", "jre-openjdk", "flatpak", "networkmanager", "wget", "curl", "python", "python-pipx",

		"gdm",
		"gnome-keyring", "gnome-session", "gnome-settings-daemon", "gnome-shell",
		"gvfs", "gvfs-afc", "gvfs-goa", "gvfs-google", "gvfs-gphoto2", "gvfs-mtp", "gvfs-nfs", "gvfs-smb",
		"gnome-logs", "gnome-font-viewer",
		"gnome-console", "gnome-control-center", "gnome-disk-utility",
		"gnome-shell-extension-appindicator",
		"nautilus", "sushi", "dconf-editor", "file-roller",
		"rygel", "tracker3-miners", "xdg-desktop-portal-gnome", "xdg-user-dirs-gtk",

		"firefox",
		"qbittorrent", "dbeaver",
		"meld", "picard", "homebank",
		"solaar", "vlc", "obs-studio", "inkscape", "ardour", "blender", "easyeffects", "steam",
		"yt-dlp",
		"wine-gecko", "wine-mono", "wine-staging",
		"bluez", "bluez-libs", "bluez-utils",
		"qt6-multimedia-ffmpeg", "webp-pixbuf-loader", "ttf-croscore", "calf", "mda.lv2", "lsp-plugins"
	],
	"aur": ["yay-bin"],
	"flatpak": ["de.haeckerfelix.Shortwave"],
	"pipx": ["gnome-extensions-cli"],
	"skip-mirror-check": false,
	"profile": "xorg",
	"services": [
		"gdm",
		"bluetooth",
		"NetworkManager"
	],
	"swap": true,
	"silent": false,
	"sys-encoding": "utf-8",
	"sys-language": "en_US",
	"timezone": "Australia/Sydney"
}