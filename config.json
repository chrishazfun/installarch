{
	"debug": false,
	"additional-repositories": ["multilib"],
	"audio": "pipewire",
	"bootloader": "grub-install",
	"hostname": "host",
	"disk_encryption": null,
	"kernels": ["linux"],
	"keyboard-layout": "us",
	"mirror-region": "Australia",
	"ntp": true,
	"no_pkg_lookups": false,
	"offline": false,
	"plugin": "https://raw.githubusercontent.com/Torxed/archinstall-aur/main/archinstall_aur/__init__.py",
	"packages": [
		"bash", "bash-completion", "git", "jre-openjdk", "flatpak", "networkmanager", "wget",

		"baobab", "gdm", "gnome-characters", "gnome-console", "gnome-control-center", "gnome-disk-utility", "gnome-font-viewer", "gnome-keyring", "gnome-logs", "gnome-menus", "gnome-session", "gnome-settings-daemon", "gnome-shell", "gnome-shell-extensions", "gnome-software", "gnome-system-monitor", "gnome-text-editor", "gnome-user-share", "grilo-plugins", "gvfs", "gvfs-afc", "gvfs-google", "gvfs-mtp", "gvfs-nfs", "gvfs-smb", "nautilus", "rygel", "sushi", "tracker3-miners", "xdg-desktop-portal-gnome", "xdg-user-dirs-gtk", "dconf-editor", "evolution", "file-roller", "gedit", "glade", "gnome-boxes", "gnome-builder", "gnome-connections", "gnome-sound-recorder", "simple-scan", "sysprof",

		"kid3-qt", "meld", "picard", "homebank",
		"solaar", "vlc", "obs-studio", "inkscape", "ardour", "blender",
		"bluez", "bluez-libs", "bluez-utils",
		"wine-gecko", "wine-mono", "wine-staging",
		"steam",
		"btop", "yt-dlp",
		"qt6-multimedia-ffmpeg", "webp-pixbuf-loader", "ttf-croscore",

		"yay-bin", "firefox", "microsoft-edge-stable-bin"
	],
	"skip-mirror-check": false,
	"profile": "xorg",
	"services": [
		"lightdm",
		"bluetooth",
		"NetworkManager"
	],
	"swap": true,
	"silent": false,
	"sys-encoding": "utf-8",
	"sys-language": "en_US",
	"timezone": "Australia/Sydney"
}