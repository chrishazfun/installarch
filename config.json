{
	"additional-repositories": [
		"multilib"
	],
	"custom-commands": [
		"sed -i 's/^#ParallelDownloads = 5/ParallelDownloads = 25/' /etc/pacman.conf",
		"sed -i 's/^#IgnorePkg   =/IgnorePkg=xterm/' /etc/pacman.conf",
		"sed -i 's/^#Color/Color/' /etc/pacman.conf"
	],
	"archinstall-language": "English",
	"audio_config": {
		"audio": "pipewire"
	},
	"bootloader": "Grub",
	"config_version": "2.6.3",
	"debug": false,
	"hostname": "changethisplease",
	"kernels": [
		"linux"
	],
	"locale_config": {
		"kb_layout": "us",
		"sys_enc": "UTF-8",
		"sys_lang": "en_US"
	},
	"mirror_config": {
		"custom_mirrors": [],
		"mirror_regions": {
			"Worldwide": [
				"https://geo.mirror.pkgbuild.com/$repo/os/$arch"
			]
		}
	},
	"network_config": {
		"type": "nm"
	},
	"no_pkg_lookups": false,
	"ntp": true,
	"offline": false,
	"packages": [
		"bash", "bash-completion", "nano", "git", "jre-openjdk", "flatpak", "wget", "curl", "python", "python-pipx",
		"gdm", "gnome-clocks", "gnome-characters", "gnome-color-manager", "gnome-console", "gnome-control-center", "gnome-disk-utility", "gnome-font-viewer", "gnome-music", "simple-scan", "snapshot", "tecla", "d-spy", "dconf-editor", "file-roller", "glade", "gnome-builder", "gnome-notes", "gnome-sound-recorder", "polari", "seahorse", "tali",
		"gnome-keyring", "gnome-logs", "gnome-menus", "gnome-session", "gnome-settings-daemon", "gnome-shell", "gnome-user-share", "grilo-plugins", "gnome-software", "gnome-system-monitor", "gnome-text-editor", "loupe", "nautilus", "rygel", "tracker3-miners",
		"gvfs", "gvfs-afc", "gvfs-goa", "gvfs-google", "gvfs-gphoto2", "gvfs-mtp", "gvfs-nfs", "gvfs-smb",
		"xdg-desktop-portal-gnome", "xdg-user-dirs-gtk",
		"yt-dlp", "distrobox",
		"toot",
		"streamlink",
		"wine-gecko", "wine-mono", "wine-staging",
		"firefox", "qbittorrent",
		"meld", "picard", "homebank",
		"mpv", "obs-studio", "gimp", "ardour", "blender", "steam",
		"bluez", "bluez-libs", "bluez-utils",
		"qt6-multimedia-ffmpeg", "webp-pixbuf-loader", "ttf-croscore", "ffmpegthumbnailer"
	],
	"services": [
		"gdm",
		"bluetooth"
	],
	"parallel downloads": 25,
	"profile_config": {
		"gfx_driver": "All open-source",
		"greeter": null,
		"profile": {
			"custom_settings": {},
			"details": [],
			"main": "Xorg"
		}
	},
	"script": "guided",
	"silent": false,
	"swap": true,
	"skip-mirror-check": true,
	"timezone": "Australia/Sydney",
	"version": "2.6.3"
}